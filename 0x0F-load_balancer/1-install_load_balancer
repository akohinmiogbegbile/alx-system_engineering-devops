# edit config file
server_config=\
"
frontend  akohinmiogbegbile_frontend
        bind *:80
        mode http
        default_backend besthor_backend
backend akohinmiogbegbile_backend
        balance roundrobin
        server 301824-web-01 52.23.177.223:80 check
        server 301824-web-02 100.26.210.47:80 check
"
echo "$server_config" | sudo tee -a /etc/haproxy/haproxy.cfg
